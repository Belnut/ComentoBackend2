<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mumuni.springboot_web.hello">
    <select id="selectAllUsers" parameterType="map" resultType="TestVO">
        SELECT id, name, password, level, login, recommend
        FROM users WHERE name like #{userName}
    </select>
</mapper>